<!DOCTYPE html>
<html lang="pt-BR">
<%- include('./partials/head.ejs')%>
<body>
    <%- include('./partials/nav.ejs') %> 
    <form action="api/ad/logout" method="post" class="form-submit">
        <button type="submit" title="Logout"><img src="logoutimg.png" alt="logout" class="img"></button>
    </form>
    <div class="resultado">
        <% if (visitante.length > 0) { %>
            <table class="table-result">
                <tr>
                    <th>DATA/HORA</th>
                    <th>NOME</th>
                    <th>CPF</th>
                    <th>HOSTNAME</th>
                </tr>
                <% visitante.forEach(visitanteExist => { %>
                    <tr>
                        <td style="width: 300px; text-align: center;"><%= visitanteExist.STAMP.replace('BRT', '').replace('at', '') %></td>
                        <td><%= visitanteExist.NOME %></h3>
                            <td style="text-align: right;"><%= visitanteExist.CPF %></td>                           
                            <td style="text-align: right;"><%= visitanteExist.HOST %></td>
                        </tr>
                        <% }) %>
                        <%} else { %>
                            <p>Não foi possivel encontrar o histórico de Pessoas </p>
                            <%}%>
                        </table>
                    </div>
                    <%- include('./partials/footer.ejs') %> 
                </body>
                </html>